{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html"
        ],
        "versionedFiles": [
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "versionedFiles": [
          "**/*.svg",
          "**/*.png",
          "**/*.woff",
          "**/*.woff2",
          "**/*.eot",
          "**/*.ttf"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "performance-cache-long-time",
      "urls": [
        "https://image.tmdb.org/t/p/w342/*.jpg",
        "https://image.tmdb.org/t/p/w342//*.jpg",
        "https://image.tmdb.org/t/p/w500/*.jpg",
        "https://image.tmdb.org/t/p/w500//*.jpg"
      ],
      "cacheConfig": {
        "maxSize": 500,
        "maxAge": "15d",
        "strategy": "performance"
      }
    },
    {
      "name": "performance-cache-short-time",
      "urls": [
        "https://api.themoviedb.org/*",
        "*.js",
        "*.css",
        "*.svg",
        "*.png",
        "*.woff",
        "*.woff2",
        "*.eot",
        "*.ttf"
      ],
      "cacheConfig": {
        "maxSize": 500,
        "maxAge": "1h",
        "strategy": "performance"
      }
    }
  ]
}
